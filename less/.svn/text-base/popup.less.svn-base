/* \\\\\\\\\\\\\\\\\    MODAL CODE   ///////////////////////////////// */
.overlay {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  display: none;
}

.modal {
  display: none;
  background: #eee;
  // padding: 0 20px 20px;
  overflow: auto;
  z-index: 1001;
  position: absolute;
  width: 500px;
  min-height: 200px;
  box-shadow: 5px 5px #000;
  color: #000;

  p {
    font-size: 14px;
    letter-spacing: 0px;
    margin-top: 30px;
    margin-bottom: 28px;
  }

  b {
    font-weight: bold;
  }

  object {
    visibility: visible;
    border: 2px solid #000;
    // margin-top: 21px;
    // border: 7px solid #000;
  }
}

.modalLink {
  position: absolute;
  left: -3000px;
}
/* \\\\\\\\\\\\\\\\\    END MODAL CODE   ///////////////////////////////// */


.videotuto {
  font-family: BradBun;
  color: #9cbcbe;
  width: 652px !important;
  height: 367px;
  position: absolute;
  top: 47px;
  left: -3px;
  right: 0;
  bottom: 0;
  margin: auto;
}


.bt-close {
  z-index: 10000 !important;
}