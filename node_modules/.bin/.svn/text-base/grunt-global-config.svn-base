#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../grunt-global-config/bin/grunt-global-config" "$@"
  ret=$?
else 
  node  "$basedir/../grunt-global-config/bin/grunt-global-config" "$@"
  ret=$?
fi
exit $ret
