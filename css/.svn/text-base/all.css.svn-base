/****** Mixins *********************/
.reset-button {
  background-color: transparent;
  border: 0;
  cursor: pointer;
}
.text-shadow {
  text-shadow: .1rem .1rem 0 #000;
}
.bg-discovery {
  background-image: linear-gradient(bottom, #3d3d3d 0%, #212121 50%, #303030 50%, #010101 100%);
  background-image: -o-linear-gradient(bottom, #3d3d3d 0%, #212121 50%, #303030 50%, #010101 100%);
  background-image: -moz-linear-gradient(bottom, #3d3d3d 0%, #212121 50%, #303030 50%, #010101 100%);
  background-image: -webkit-linear-gradient(bottom, #3d3d3d 0%, #212121 50%, #303030 50%, #010101 100%);
  background-image: -ms-linear-gradient(bottom, #3d3d3d 0%, #212121 50%, #303030 50%, #010101 100%);
}
/* Font */
.font-title {
  font-family: 'Oswald', sans-serif;
  letter-spacing: .1rem;
  text-transform: uppercase;
}
.font-current {
  font-family: 'Pontano Sans', sans-serif;
  letter-spacing: .1rem;
}
.btn-shadow {
  background: -webkit-gradient(-45deg, #000000 25%, transparent 0, transparent 50%, #000000 0, #000000 75%, transparent 0);
  background: -ms-linear-gradient(-45deg, #000000 25%, transparent 0, transparent 50%, #000000 0, #000000 75%, transparent 0);
  background: -moz-linear-gradient(-45deg, #000000 25%, transparent 0, transparent 50%, #000000 0, #000000 75%, transparent 0);
  background: linear-gradient(-45deg, #000000 25%, transparent 0, transparent 50%, #000000 0, #000000 75%, transparent 0);
  background-size: 4px 4px;
  opacity: .7;
}
